    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-validator": "^6.2.0",
    "graphql": "^14.5.8",
    "link-module-alias": "^1.2.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.12",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.1.2",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.18.2"
  },
  "_moduleAliases": {
    "~": ".",
    "~controllers": "./src/controllers/",
    "~graphql": "./src/graphql/",
